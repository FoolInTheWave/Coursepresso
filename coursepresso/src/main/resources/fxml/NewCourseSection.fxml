<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" 
            maxHeight="-Infinity" 
            maxWidth="-Infinity" 
            minHeight="-Infinity" 
            minWidth="-Infinity" 
            prefHeight="800.0" 
            prefWidth="1280.0" 
            styleClass="mainFxmlClass" 
            xmlns="http://javafx.com/javafx/8" 
            xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="com.coursepresso.project.presenter.NewCourseSectionPresenter">
    <stylesheets>
        <URL value="@/styles/Styles.css" />
    </stylesheets>
   <children>
      <TitledPane fx:id="root" animated="false" layoutX="14.0" layoutY="14.0" prefHeight="771.0" prefWidth="1254.0" text="New Course Section">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <Label layoutX="14.0" layoutY="14.0" text="Department" />
                  <ChoiceBox fx:id="departmentChoice" layoutX="14.0" layoutY="41.0" prefHeight="39.0" prefWidth="430.0" />
                  <Label layoutX="470.0" layoutY="14.0" text="Course Number" />
                  <ChoiceBox layoutX="470.0" layoutY="41.0" prefWidth="150.0" />
                  <Label layoutX="648.0" layoutY="14.0" text="Title" />
                  <TextField fx:id="titleField" editable="false" layoutX="648.0" layoutY="41.0" prefHeight="39.0" prefWidth="590.0" />
                  <Label layoutX="14.0" layoutY="102.0" text="Instructor" />
                  <ChoiceBox fx:id="instructorChoice" layoutX="14.0" layoutY="129.0" prefHeight="39.0" prefWidth="430.0" />
                  <Label layoutX="470.0" layoutY="102.0" text="Section" />
                  <TextField fx:id="sectionField" editable="false" layoutX="470.0" layoutY="129.0" prefHeight="39.0" prefWidth="115.0" />
                  <Label layoutX="620.0" layoutY="102.0" text="Capacity" />
                  <TextField layoutX="620.0" layoutY="129.0" prefHeight="39.0" prefWidth="115.0" />
                  <Label layoutX="769.0" layoutY="102.0" text="Type" />
                  <ChoiceBox fx:id="typeChoice" layoutX="769.0" layoutY="129.0" prefWidth="150.0" />
                  <Label layoutX="955.0" layoutY="102.0" text="Term" />
                  <ChoiceBox layoutX="955.0" layoutY="129.0" prefWidth="150.0" />
                  <Label layoutX="14.0" layoutY="201.0" text="Start Date" />
                  <DatePicker fx:id="startDatePicker" layoutX="14.0" layoutY="228.0" />
                  <Label layoutX="325.0" layoutY="201.0" text="End Date" />
                  <DatePicker fx:id="endDatePicker" layoutX="325.0" layoutY="228.0" />
                  <TableView fx:id="MeetingDayTable" editable="true" layoutX="14.0" layoutY="301.0" prefHeight="320.0" prefWidth="1222.0">
                    <columns>
                      <TableColumn fx:id="startTimeColumn" prefWidth="101.0" text="Start Time" />
                      <TableColumn fx:id="endTimeColumn" minWidth="6.0" prefWidth="93.0" text="End Time" />
                      <TableColumn fx:id="roomColumn" minWidth="0.0" prefWidth="67.0" text="Room" />
                      <TableColumn fx:id="dayColumn" minWidth="6.0" prefWidth="108.0" text="Day" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <Button fx:id="submitCourseButton" layoutX="15.0" layoutY="645.0" mnemonicParsing="false" text="Submit Course" />
                  <Button fx:id="backToListingButton" layoutX="181.0" layoutY="645.0" mnemonicParsing="false" text="Back to Listing" />
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
   </children>
</AnchorPane>
